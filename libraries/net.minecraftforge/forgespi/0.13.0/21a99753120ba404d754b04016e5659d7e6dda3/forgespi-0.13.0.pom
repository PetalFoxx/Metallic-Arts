      <id>cpw</id>
      <name>cpw</name>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:git://github.com/MinecraftForge/ForgeSPI.git</connection>
    <developerConnection>scm:git:git@github.com:MinecraftForge/ForgeSPI.git</developerConnection>
    <url>https://github.com/MinecraftForge/ForgeSPI</url>
  </scm>
  <issueManagement>
